<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<T3DataStructure>
 <meta>
 	<langDisable>1</langDisable>
 </meta>
 <sheets>
  <sDEF>
   <ROOT>
    <TCEforms>
    	<sheetTitle>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_sheet_sDEF</sheetTitle>
    	<cshFile>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_csh_flexform_video.xlf</cshFile>
    </TCEforms>
    <type>array</type>
    <el>
		<switchableControllerActions>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_pluginType</label>
				<onChange>reload</onChange>
				<config>
					<type>select</type>
					<renderType>selectSingle</renderType>
					<items type="array">
						<numIndex index="0" type="array">
							<numIndex index="0">LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_pluginType.list</numIndex>
							<numIndex index="1">Video->list;Video->show</numIndex>
						</numIndex>
						<numIndex index="1" type="array">
							<numIndex index="0">LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_pluginType.single</numIndex>
							<numIndex index="1">Video->presetShow</numIndex>
						</numIndex>
						<numIndex index="2" type="array">
							<numIndex index="0">LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_pluginType.live</numIndex>
							<numIndex index="1">Video->liveStream</numIndex>
						</numIndex>
						<numIndex index="3" type="array">
							<numIndex index="0">LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_pluginType.advanced</numIndex>
							<numIndex index="1">Video->advancedShow</numIndex>
						</numIndex>
					</items>
					<maxitems>1</maxitems>
					<size>1</size>
				</config>
			</TCEforms>
		</switchableControllerActions>
		<settings.event.dateAt>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_dateAt</label>
				<displayCond>FIELD:switchableControllerActions:=:Video->list;Video->show</displayCond>
				<config>
					<type>input</type>
					<size>6</size>
					<eval>date</eval>
					<default></default>
				</config>
			</TCEforms>
		</settings.event.dateAt>
		<settings.event.dateFrom>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_dateFrom</label>
				<displayCond>FIELD:switchableControllerActions:=:Video->list;Video->show</displayCond>
				<config>
					<type>input</type>
					<size>9</size>
					<eval>datetime</eval>
					<default></default>
				</config>
			</TCEforms>
		</settings.event.dateFrom>
		<settings.event.dateTo>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_dateTo</label>
				<displayCond>FIELD:switchableControllerActions:=:Video->list;Video->show</displayCond>
				<config>
					<type>input</type>
					<size>9</size>
					<eval>datetime</eval>
					<default></default>
				</config>
			</TCEforms>
		</settings.event.dateTo>
		<settings.event.category>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_category</label>
				<displayCond>FIELD:switchableControllerActions:!IN:Video->presetShow,Video->advancedShow</displayCond>
				<config>
					<type>input</type>
					<size>50</size>
					<max>255</max>
					<eval>trim,alphanum_x</eval>
					<default></default>
				</config>
			</TCEforms>
		</settings.event.category>
		<settings.event.subCategory>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_subCategory</label>
				<displayCond>FIELD:switchableControllerActions:!IN:Video->presetShow,Video->advancedShow</displayCond>
				<config>
					<type>input</type>
					<size>50</size>
					<max>255</max>
					<eval>trim,alphanum_x</eval>
					<default></default>
				</config>
			</TCEforms>
		</settings.event.subCategory>
		<settings.event.tags>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_tags</label>
				<displayCond>FIELD:switchableControllerActions:!IN:Video->presetShow,Video->advancedShow</displayCond>
				<config>
					<type>input</type>
					<size>50</size>
					<max>255</max>
					<!-- @LOW allow only alphanum, _ and : ? -->
					<eval>trim</eval>
					<default></default>
				</config>
			</TCEforms>
		</settings.event.tags>
		<settings.playlist.hash>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_hash</label>
				<displayCond>FIELD:switchableControllerActions:=:Video->presetShow</displayCond>
				<config>
					<type>input</type>
					<size>30</size>
					<max>32</max>
					<eval>trim,alphanum,required</eval>
					<default></default>
				</config>
			</TCEforms>
		</settings.playlist.hash>
		<settings.showPid>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_showPid</label>
				<displayCond>FIELD:switchableControllerActions:=:Video->list;Video->show</displayCond>
				<config>
					<type>group</type>
					<internal_type>db</internal_type>
					<allowed>pages</allowed>
					<size>1</size>
					<maxitems>1</maxitems>
					<minitems>0</minitems>
					<show_thumbs>1</show_thumbs>
					<wizards>
						<suggest>
							<type>suggest</type>
						</suggest>
					</wizards>
				</config>
			</TCEforms>
		</settings.showPid>
		<settings.backPid>
			<TCEforms>
				<label>LLL:EXT:streamovations_vp/Resources/Private/Language/locallang_be.xml:ff_video_backPid</label>
				<displayCond>FIELD:switchableControllerActions:=:Video->list;Video->show</displayCond>
				<config>
					<type>group</type>
					<internal_type>db</internal_type>
					<allowed>pages</allowed>
					<size>1</size>
					<maxitems>1</maxitems>
					<minitems>0</minitems>
					<show_thumbs>1</show_thumbs>
					<wizards>
						<suggest>
							<type>suggest</type>
						</suggest>
					</wizards>
				</config>
			</TCEforms>
		</settings.backPid>
    </el>
   </ROOT>
  </sDEF>
 </sheets>
</T3DataStructure>